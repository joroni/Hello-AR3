<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ezartech.demo.helloezar" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello ezAR</name>
    <description>
       Minimalist ezAR hello-world app
    </description>
    <author email="info@ezartech.com" href="http://ezartech.com">
        ezAR Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="com.ezartech.ezar.videooverlay" spec="~0.2.11" />
    <engine name="ios" spec="~4.4.0" />
</widget>
